/** Estructura del player en el mapa **/
struct{
    /** coordenadas donde inicia **/
    int x=13; //coordenada en x
    int y=1; //coordenada en y
}player;

struct stats{
    string nombre;
    string seleccion="0";
    int ataque=10;
    int mvida=50;
    int vida=mvida;
    int defensa=5;
    int velocidad=50;
    int magia=50;
    bool vivo=true;
    bool amigo=true;

    void (*attack)(stats *,int,int,int&,int&);
}Hero,Karsus,Aballister;

